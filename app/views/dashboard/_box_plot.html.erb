
    <div id="<%= id %>"></div>
    <script>
        var options = {
            series: [
                {
                    name: 'box',
                    type: 'boxPlot',
                    data: <%= raw dados[0].to_json %>
                },
                {
                    name: 'outliers',
                    type: 'scatter',
                    data: <%= raw dados[1].to_json %>
                }
            ],
            chart: {
                type: 'boxPlot',
                height: 350
            },
            colors: ['#008FFB', '#FEB019'],
            title: {
                text: '<%= name_x.upcase %>',
                align: 'left'
            },
            xaxis: {
            },
            tooltip: {
                shared: false,
                intersect: true
            }
        };

        var chart = new ApexCharts(document.querySelector("#<%= id %>"), options);
        chart.render();
    </script>
