<%= turbo_frame_tag 'render_table_dashbord' do %>

  <div class="col-md-12 mb-5" style="text-align: center;'">
    <%= link_to dashboard_index_path(type_data: true, partial: 'data_menu'),
                class: "btn btn-primary btn-block mt-3 mr-2 button-dashbord button-active-color #{'button-active' if @type_data}",
                data: { turbo_frame: 'render_table_dashbord' } do %>
      Dados <br> Diarios
    <% end %>

    <%= link_to dashboard_index_path(type_data: false, partial: 'data_menu'),
                class: "btn btn-primary btn-block mt-3 mr-2 button-dashbord button-active-color #{'button-active' unless @type_data}",
                data: { turbo_frame: 'render_table_dashbord' } do %>
      Dados <br> por Per√≠odo
    <% end %>
  </div>

  <% if @type_data %>
    <%= render 'daily_data' %>
  <% else %>
    <%= render 'range_data' %>
  <% end %>

<% end %>